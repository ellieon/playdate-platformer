{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "diagnostics.globals": ["import",
        "Player",
        "GameScene",
        "Ability",
        "Spike",
        "Spikeball",
        "AnimatedSprite",
        "State",
        "StateMachine",
        "RunState",
        "IdleState",
        "JumpState",
        "DashState",
        "Actor",
        "PlayerState",
        "AirState",
        "PlayerInputHandler",
        "GroundState",
        "FallState",
        "Signal",
        "Timeline",
        "ItemPickupDialog",
        "EventHandler",
        "FreezeState",
        "PlayerCamera",
        "LevelTransitionFade",
        "MapScene",
        "Scene",
        "SceneManager"],
    "diagnostics.severity": { "duplicate-set-field": "Hint" },
    "format.defaultConfig": { "indent_style": "space", "indent_size": "4" },
    "runtime.builtin": { "io": "disable", "os": "disable", "package": "disable" },
    "runtime.nonstandardSymbol": ["+=", "-=", "*=", "/=", "//=", "%=", "<<=", ">>=", "&=", "|=", "^="],
    "runtime.version": "Lua 5.4",
    "workspace.preloadFileSize": 1000,
    "workspace.library": ["~/Developer/playdate-luacats"]
  }